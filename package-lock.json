{
    "name": "main",
    "version": "1.0.0",
    "lockfileVersion": 2,
    "requires": true,
    "packages": {
        "": {
            "name": "main",
            "version": "1.0.0",
            "workspaces": [
                "baz",
                "@packages/*"
            ]
        },
        "@packages/bar": {
            "version": "1.0.0"
        },
        "@packages/foo": {
            "version": "1.0.0",
            "dependencies": {
                "@packages/bar": "1.0.0"
            }
        },
        "baz": {
            "version": "1.0.0",
            "dependencies": {
                "@packages/bar": "1.0.0",
                "@packages/foo": "1.0.0"
            }
        },
        "node_modules/@packages/bar": {
            "resolved": "@packages/bar",
            "link": true
        },
        "node_modules/@packages/foo": {
            "resolved": "@packages/foo",
            "link": true
        },
        "node_modules/baz": {
            "resolved": "baz",
            "link": true
        }
    },
    "dependencies": {
        "@packages/bar": {
            "version": "file:@packages/bar"
        },
        "@packages/foo": {
            "version": "file:@packages/foo",
            "requires": {
                "@packages/bar": "1.0.0"
            }
        },
        "baz": {
            "version": "file:baz",
            "requires": {
                "@packages/bar": "1.0.0",
                "@packages/foo": "1.0.0"
            }
        }
    }
}
